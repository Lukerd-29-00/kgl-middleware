{
  "name": "kgl-middleware",
  "version": "1.0.0",
  "description": "MiddleWare software for KGL",
  "main": "./src/start.ts",
  "scripts": {
    "test": "jest --watchAll --forceExit",
    "test:cov": "jest --coverage --watchAll --forceExit",
    "start": "ts-node ./src/start.ts",
    "lint": "eslint ./src ./test --ext .ts --max-warnings 0",
    "lint:fix": "eslint ./src ./test --ext .ts --max-warnings 0 --fix",
    "setup": "ts-node ./setup/setup.ts --server http://localhost:7200 --config ./test/testRepository.ttl -r LearnerRecordsTesting -o ./setup/input/testOntology.ttl --content ./setup/input/testContent.ttl"
  },
  "author": "Casey Rock",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "joi": "^17.4.2",
    "morgan": "^1.10.0",
    "node-fetch": "^2.1.0",
    "typescript": "^4.4.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.7",
    "@types/jest": "^27.0.2",
    "@types/node-fetch": "^2.1.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.2.0",
    "form-data": "^4.0.0",
    "jest": "^27.3.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
